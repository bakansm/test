{
  "manifestVersion": "1",
  "id": "clinkai-test",
  "title": "Test ClinkAI",
  "description": "Test Pluto SDK request on ClinkAI",
  "prepareUrl": "https://app.clinkai.xyz/auth/login",
  "request": {
    "url": "https://api-beta.clinkai.xyz/api/users/me",
    "method": "GET",
    "headers": {
      "Authorization": "Bearer <% authToken %>",
      "Content-Type": "application/json"
    },
  },
  "response": {
    "status": "200",
    "headers": {
      "content-type": "application/json"
    },
    "body": {
      "json": ["wallet_address", "username", "name", "clink_points"]
    }
  }
}
