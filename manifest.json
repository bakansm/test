{
  "manifestVersion": "3",
  "id": "2e37332c-1de6-46f0-890b-535dfa6273aa",
  "title": "Test ClinkAI",
  "description": "Test Pluto SDK request on ClinkAI",
  "prepareUrl": "https://app-dev.clinkai.xyz/auth/login",
  "request": {
    "url": "https://api-dev.clinkai.xyz/api/users/me",
    "method": "GET",
    "headers": {
      "Authorization": "Bearer <% authToken %>",
      "Content-Type": "application/json"
    },
  },
  "response": {
    "status": "200",
    "headers": {
      "content-type": "application/json"
    },
    "body": {
      "json": ["wallet_address", "username", "name", "clink_points"]
    }
  }
}
